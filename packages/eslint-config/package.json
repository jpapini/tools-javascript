        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "8.0.0"
    },
    "devDependencies": {
        "eslint": "8.20.0",
        "jest": "28.1.3",
        "prettier": "2.7.1",
        "typescript": "4.7.4"
    },
    "peerDependencies": {
        "eslint": ">=8",
        "jest": "*",
        "prettier": "*",
        "typescript": "*"
    },
    "peerDependenciesMeta": {
        "eslint": {
            "optional": true
        },
        "jest": {
            "optional": true
        },
        "prettier": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
