{
    "name": "@jpapini/eslint-config",
    "version": "0.1.2",
    "description": "Shared ESLint configuration for JavaScript projects.",
    "keywords": [
        "eslint",
        "eslint-config",
        "typescript",
        "javascript",
        "jest",
        "import",
        "prettier"
    ],
    "homepage": "https://github.com/jpapini/tools-javascript/tree/main/packages/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/jpapini/tools-javascript/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jpapini/tools-javascript.git",
        "directory": "packages/eslint-config"
    },
    "license": "MIT",
    "author": "Julien Papini <julien.papini@gmail.com> (https://github.com/jpapini)",
    "main": "index.js",
    "files": [
        "CHANGELOG.md",
        "import.js",
        "index.js",
        "jest.js",
        "LICENSE.md",
        "README.md",
        "typescript.js"
    ],
    "scripts": {
        "clean": "rimraf .turbo",
        "reset": "pnpm run clean; rimraf node_modules"
    },
    "dependencies": {
        "@rushstack/eslint-patch": "1.1.4",
        "@typescript-eslint/eslint-plugin": "5.31.0",
        "@typescript-eslint/parser": "5.31.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
